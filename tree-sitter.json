{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "masm",
      "camelcase": "MASM",
      "title": "Miden Assembly",
      "scope": "source.masm",
      "file-types": ["masm"],
      "injection-regex": "^masm$",
      "class-name": "TreeSitterMasm",
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Miden Assembly grammar for tree-sitter",
    "authors": [
      {
        "name": "Paul Schoenfelder"
      }
    ],
    "links": {
      "repository": "https://github.com/0xMiden/tree-sitter-masm"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
